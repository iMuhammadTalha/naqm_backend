CREATE TABLE public.forget_password
(
    id bigserial NOT NULL,
    user_id bigserial NOT NULL,
    token text NOT NULL,
    PRIMARY KEY (id),
    FOREIGN KEY (user_id)
        REFERENCES public.users (id) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE CASCADE
)
WITH (
    OIDS = FALSE
);

ALTER TABLE public.forget_password
    OWNER to postgres;